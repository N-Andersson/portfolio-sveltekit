    "autoprefixer": "^10.4.0",
    "cssnano": "^5.0.10",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "patch-package": "^6.4.7",
    "postcss": "^8.3.11",
    "postcss-load-config": "^3.1.0",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
